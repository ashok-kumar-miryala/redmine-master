<% if(!@venues.blank?) && (@venues["places"]!=[])%>
<%= results_app_params(:bus_name =>"Foursquare",:address => @venues["places"][0].location["address"]+","+@venues["places"][0].location["city"]+","+@venues["places"][0].location["state"]+","+@venues["places"][0].location["country"]+","+@venues["places"][0].location["postalCode"],:category =>@venues["places"][0].categories,:web_site =>@venues["places"][0].json["url"],:description=>"",:photos=>@venues["places"][0].icon,:offer=>@venues["places"][0].json["specials"],:total_revenue=>"",:fount_or_not=>"yes",:i=>i+=1, :image_url=>"foursquare.jpg")%>
