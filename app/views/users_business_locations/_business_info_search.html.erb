<% if(@yelp_results=="OK") && (@response_results["businesses"]!=[]) %>
<%= results_app_params(:bus_name => "Yelp", :address => @response_results["businesses"][0]["address1"]+","+@response_results["businesses"][0]["city"]+","+@response_results["businesses"][0]["state"], :category => @response_results["businesses"][0]["categories"],:web_site => @response_results["businesses"][0]["url"], :description=>"",:photos=>@response_results["businesses"][0]["url"],:offer=>"",:total_revenue=>@response_results["businesses"][0]["review_count"],:fount_or_not=>"yes",:i=>i,:image_url=>"yelp.jpg")%>
