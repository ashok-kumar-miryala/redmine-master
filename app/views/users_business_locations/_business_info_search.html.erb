<%= results_app_params(:bus_name =>"Yelp", :address=>"",:category=>"",:web_site=>"",:description=>"",:photos=>"",:offer=>"",:total_revenue=>"",:fount_or_not=>"",:i=>i,:image_url=>"yelp.jpg")%>
