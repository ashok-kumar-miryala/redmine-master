<%= results_app_params(:bus_name =>"Foursquare",:address =>"",:category =>"",:web_site =>"",:description=>"",:photos=>"",:offer=>"",:total_revenue=>"",:fount_or_not=>"",:i=>i+=1, :image_url=>"foursquare.jpg")%>
<% end %>
		
<% if !@yahoo_results.blank? && !@yahoo_results[0].title.blank? %>
<%= results_app_params(:bus_name =>"Yahoo",:address =>@yahoo_results[0].address+","+@yahoo_results[0].city+","+@yahoo_results[0].state+","+@yahoo_results[0].phone,:category =>@yahoo_results[0].categories,:web_site =>@yahoo_results[0].url,:description=>"",:photos=>@yahoo_results[0].map_url,:offer=>"",:total_revenue=>@yahoo_results[0].total_reviews,:fount_or_not=>"yes",:i=>i+=1, :image_url=>"yahoo.jpg")%>
		
